{
  "name": "component-parser",
  "version": "0.1.0",
  "description": "Commands to extract component CSS from partial HTML file.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kent013/component-parser.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kent013/component-parser/issues"
  },
  "homepage": "https://github.com/kent013/component-parser#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.7.1",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
}
